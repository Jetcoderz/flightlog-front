{
  "expo": {
    "name": "Flightlog",
    "slug": "flightlog",
    "version": "1.1.0",
    "icon": "./assets/icon.png",
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jetcoderz.flightlog",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to allow the user to capture images and attach them to the flightlog.",
        "NSPhotoLibraryUsageDescription": "This app accesses the photo library to attach user selected images to the flightlog."
      }
    },
    "android": {
      "icon": "./assets/playstoreicon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/planeNcloudtiny.png",
        "backgroundImage": "./assets/bg.png"
      },
      "splash": {
        "image": "./assets/planecloudsm.png",
        "backgroundColor": "#0e2b77"
      },
      "package": "com.jetcoderz.flightlog",
      "versionCode": 10,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA5df97hjQlblBQcaZMeRhTVleU1RlbkV8"
        }
      }
    }
  }
